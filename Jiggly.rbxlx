	_inputTable: PubTypes.CanBeState&lt;{ [KI]: KO }>,
	_oldInputTable: { [KI]: KO },
	_outputTable: { [KO]: any },
	_keyOIMap: { [KO]: KI },
	_keyIOMap: { [KI]: KO },
